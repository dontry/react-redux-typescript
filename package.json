{
  "name": "react-redux-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "formik": "^1.5.7",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.1",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch --verbose",
    "test:debug": "node --inspect-brk jest --runInBand --watch",
    "test:coverage": "jest --notify --coverage",
    "lint": "tslint src/**/*.ts{,?}",
    "prettier": "prettier  \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "format": "npm run prettier -- --write",
    "validate-staged": "lint-staged",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/redux": "^3.6.0",
    "@types/reselect": "^2.2.0",
    "@types/styled-components": "^4.1.15",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.13",
    "cypress-testing-library": "^4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "faker": "^4.1.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^2.0.0",
    "jest-dom": "^3.4.0",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.9.0",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
